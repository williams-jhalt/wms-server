{% extends 'base.html.twig' %}

{% block body %}

    <div class="col-lg-12">
        <h1 class="page-header">Packer Information</h1>
    </div>

    <dl class="dl-horizontal">

        <dt>Packer</dt>
        <dd>{{ userId }}</dd>

        <dt>Average Packages Per Day</dt>
        <dd>{{ averagePackagesPerDay }}</dd>

        <dt>Average Items Per Day</dt>
        <dd>{{ averageItemsPerDay }}</dd>

    </dl>

    <p><small>Averages calculated over one month, assuming 5 day weeks, 30 day month.</small></p>

{% endblock %}
